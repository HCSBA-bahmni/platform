{
  "id": "dashboard-visualiser-kibana",
  "name": "Dashboard Visualiser Kibana",
  "description": "A dashboard to interpret the data from the ElasticSearch data store",
  "type": "infrastructure",
  "version": "0.0.1",
  "dependencies": ["analytics-datastore-elastic-search", "reverse-proxy-nginx"],
  "environmentVariables": {
    "ELASTICSEARCH_URL": "http://analytics-datastore-elastic-search:9200",
    "ELASTICSEARCH_HOSTS": "http://analytics-datastore-elastic-search:9200",
    "ELASTICSEARCH_USERNAME": "kibana_system",
    "ELASTICSEARCH_PASSWORD": "dev_password_only",
    "KIBANA_INSTANCES": 1,
    "KIBANA_PASSWORD": "dev_password_only",
    "KIBANA_USERNAME": "elastic",
    "KIBANA_CONFIG_FILE": "kibana-export.ndjson",
    "KIBANA_SSL": "true",
    "KIBANA_MEMORY_LIMIT": "3G",
    "KIBANA_MEMORY_RESERVE": "500M"
  }
}
